{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime", [
    "component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      },
      {
        "libraryName": "vue-antd-ui",
        "style": true
      }
    ]
  ]],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
